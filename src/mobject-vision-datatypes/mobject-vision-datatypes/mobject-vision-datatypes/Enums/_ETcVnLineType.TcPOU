    <Method Name="Clone" Id="{e367fe98-5ee3-4822-9518-2f8930e16523}">
      <Declaration><![CDATA[METHOD PUBLIC Clone : I_Datatype
VAR
    pClone : POINTER TO _ETcVnLineType;
END_VAR
]]></Declaration>
